                        <td>
                            <a asp-action="Edit" asp-route-id="@item.CCFreelServicesId">Edit</a> |
                            <a asp-action="Details" asp-route-id="@item.CCFreelServicesId">Details</a> |
                            <a asp-action="Delete" asp-route-id="@item.CCFreelServicesId">Delete</a>
