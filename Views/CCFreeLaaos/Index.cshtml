                    </tr>
                }
            </tbody>
        </table>



    </div>
</div>




@await Html.PartialAsync("_FreeLaarosCreateModalPartial", new CCFreelServices())

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}