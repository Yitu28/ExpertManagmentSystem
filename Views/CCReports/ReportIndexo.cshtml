                                    else if (@value.ToString() == CCPDecissionBreTypes.ተጨማሪ_ማስረጃ_የተጠየቀበት.ToString())
                                    {
                                        <td>
                                            @Model.Where(a => a.FreelCategory.Value == FreelCategory.ቀጥታክስመልስ || a.FreelCategory.Value == FreelCategory.ቀጥታክስአመልካች).Where(a => a.CCPDecissionTypes.ToString() == CCPDecissionBreTypes.ተጨማሪ_ማስረጃ_የተጠየቀበት.ToString()).Where(a => a.CCPDecissionStatus == 1).Sum(a => a.Amountincarie)
                                        </td>
                                    }
                                    else if (@value.ToString() == CCPDecissionBreTypes.ከክስ_በፊት_በድርድር_በስምምነት_የተቋጨ.ToString())
                                    {
                                        <td>
                                            @Model.Where(a => a.FreelCategory.Value == FreelCategory.ቀጥታክስመልስ || a.FreelCategory.Value == FreelCategory.ቀጥታክስአመልካች).Where(a => a.CCPDecissionTypes.ToString() == CCPDecissionBreTypes.ከክስ_በፊት_በድርድር_በስምምነት_የተቋጨ.ToString()).Where(a => a.CCPDecissionStatus == 1).Sum(a => a.Amountincarie)
                                        </td>
                                    }
                                    else
                                    {
                                        <td>
                                            0
                                        </td>

                                    }


                                </tr>

                                p++;
                            }






                        </tbody>
                    </table>
                </div>
            </div>
            <!-- /.card -->
        </div>
    </div>
}
