@model IEnumerable<ExpertManagmentSystem.Models.CivilCaseModels.CCFreeLegServiceFollowup>
@{
    ViewData["Title"] = "CCFreeLegalServiceFollowupIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card">
