        @*  <a asp-action="Create" class="btn btn-success float-right btn-circle  text-light btn-sm">አዲስ ቢሮ </a> *@
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <table class="table">
            <thead>
                <tr>
                    <th>
                        የዐ/ህ/መ/ቁጥር
                    </th>
                    <th>
                        የፍ/ቤት/መ/ቁጥር
                    </th>
                    <th>
                        ይግባኝ ባይ
