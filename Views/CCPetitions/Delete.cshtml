                        <span asp-validation-for="CCPeDecissionTypes" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-4">
                        <label asp-for="PetitionPrespons" class="control-label"></label>
                        @Html.DropDownList("PetitionPrespons", Html.GetEnumSelectList<Petitionrespons>(), " የአቤቱታው የቀረበበትን ሁኔታ ይምረጡ ", new { @class = "form-control" })
                        <span asp-validation-for="PetitionPrespons" class="text-danger"></span>
                    </div>
                    <div class="form-group  col-md-4">
                        <label asp-for="PeAssignto" class="control-label"></label>
                        <input asp-for="PeAssignto" class="form-control" disabled />
                        <span asp-validation-for="PeAssignto" class="text-danger"></span>
                    </div>
                    <div class="form-group  col-md-4">
                        <label asp-for="PeAssignto" class="control-label"></label>
                        <input asp-for="PeAssignto" class="form-control" disabled />
                        <span asp-validation-for="PeAssignto" class="text-danger"></span>
                    </div>
                </div>
                <form asp-action="Delete">
                    <input type="hidden" asp-for="CCPetitionId" />
                    <a asp-action="Index" class="btn btn-success"> ወደ ዋናው ተመለስ </a>
                    <input type="submit" value="ይሰረዝ" class="btn btn-danger float-right" />
                </form>
            </div>
        </div>
        <!-- /.card -->
    </div>
</div>