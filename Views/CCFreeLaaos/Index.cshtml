                        <td>
                            @Html.DisplayFor(modelItem => item.Gender)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.age)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SupportType)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Doo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.typesofIssue)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.apsm)
