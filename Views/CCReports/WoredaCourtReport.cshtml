                                            @Model.Where(x => x.CCFreelServices.CustomerCategory.Value == CustomerCategory.የመንግስት || x.CCFreelServices.CustomerCategory.Value == CustomerCategory.ነፃ).Where(a => a.CCFreelServices.FreelCategory.Value == FreelCategory.ሰበርተጠሪ || a.CCFreelServices.FreelCategory.Value == FreelCategory.ሰበርአመልካች).Where(d => d.DecisionStatus.ToString() == TypeofDecision.በፍርድ_ቤት_በቀጠሮ_የዞረ.ToString()).Where(a => a.CCFreelServices.CCPDecissionStatus == 1).Sum(a => a.CCFreelServices.Amountincarie)
                                        </td>
                                    }
                                    else if (@value.ToString() == TypeofDecision.ሌሎች_የፍርድ_ቤት_ውሳኔዎች.ToString())
                                    {
                                        <td>
                                            @Model.Where(x => x.CCFreelServices.CustomerCategory.Value == CustomerCategory.የመንግስት || x.CCFreelServices.CustomerCategory.Value == CustomerCategory.ነፃ).Where(a => a.CCFreelServices.FreelCategory.Value == FreelCategory.ሰበርተጠሪ || a.CCFreelServices.FreelCategory.Value == FreelCategory.ሰበርአመልካች).Where(d => d.DecisionStatus.ToString() == TypeofDecision.ሌሎች_የፍርድ_ቤት_ውሳኔዎች.ToString()).Where(a => a.CCFreelServices.CCPDecissionStatus == 1).Sum(a => a.CCFreelServices.Amountincarie)
                                        </td>
                                    }
                                    else
                                    {
                                        <td>0</td>
                                    }
                                </tr>
                                j++;
                            }

                        </tbody>
                    </table>
                </div>
            </div>
            <!-- /.card -->
        </div>
    </div>
}
