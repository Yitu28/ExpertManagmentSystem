@using ExpertManagmentSystem.Enums
@model ExpertManagmentSystem.Models.CivilCaseModels.CCdlt

<div class="modal fade" id="addCCdltmodals" aria-labelledby="addCCdltLabel" aria-hidden="true">
    <div class="modal-dialog ">
        <div class="modal-content" style="width:850px">
            <div class="card card-success">
                <div class="card-header">
                    <h3 class="card-title"> ለጠበቃ የተመራ </h3>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true" style="color:white;">×</span></button>
                </div>
                <div class="card-body">
                    <form asp-action="Create" method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label asp-for="DltFileNo" class="control-label"></label>
                            <input asp-for="DltFileNo" class="form-control" />
                            <span asp-validation-for="DltFileNo" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label asp-for="DltRecorNo" class="control-label"></label>
                            <input asp-for="DltRecorNo" class="form-control" />
                            <span asp-validation-for="DltRecorNo" class="text-danger"></span>
                        </div>
                  
                        <div class="form-group col-md-4">
                            <label asp-for="DltApplicant" class="control-label"></label>
                            <input asp-for="DltApplicant" class="form-control" />
                            <span asp-validation-for="DltApplicant" class="text-danger"></span>
                        </div>
                        </div>
                        <div class="row">
                        <div class="form-group col-md-4">
                            <label asp-for="DltResponder" class="control-label"></label>
                            <input asp-for="DltResponder" class="form-control" />
                            <span asp-validation-for="DltResponder" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label asp-for="DltGender" class="control-label"></label>
                            @* <input asp-for="DltGender" class="form-control" /> *@
                            @Html.DropDownList("DltGender", Html.GetEnumSelectList<Gender>(), " ነፃ የህግ ድጋፍ ፈላጊ ፆታ ይምረጡ ", new { @class = "form-control" })
                            <span asp-validation-for="DltGender" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label asp-for="DltAge" class="control-label"></label>
                            <input asp-for="DltAge" class="form-control" />
                            <span asp-validation-for="DltAge" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label asp-for="DltSupportType" class="control-label"></label>
                            @* <input asp-for="DltSupportType" class="form-control" /> *@
                                @Html.DropDownList("DltSupportType", Html.GetEnumSelectList<SupportType>(), " ነፃ የህግ ድጋፍ የማህበረሰብ ክፍል  ይምረጡ ", new { @class = "form-control" })
                            <span asp-validation-for="DltSupportType" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label asp-for="DltDoo" class="control-label"></label>
                            <input type="Date" asp-for="DltDoo" class="form-control" />
                            <span asp-validation-for="DltDoo" class="text-danger"></span>
                        </div>
                   
                        <div class="form-group col-md-4">
                            <label asp-for="DlttypesofIssue" class="control-label"></label>
                            <input asp-for="DlttypesofIssue" class="form-control" />
                            <span asp-validation-for="DlttypesofIssue" class="text-danger"></span>
                        </div>
                        </div>
                        <div class="row">
                        <div class="form-group col-md-4">
                            <label asp-for="DltAmountBirr" class="control-label"></label>
                            <input asp-for="DltAmountBirr" class="form-control" />
                            <span asp-validation-for="DltAmountBirr" class="text-danger"></span>
                        </div>
                    
                        <div class="form-group col-md-4">
                            <label asp-for="DltAmountKarie" class="control-label"></label>
                            <input asp-for="DltAmountKarie" class="form-control" />
                            <span asp-validation-for="DltAmountKarie" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label asp-for="DltAddressZone" class="control-label"></label>
                            <input asp-for="DltAddressZone" class="form-control" />
                            <span asp-validation-for="DltAddressZone" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md-4">
                            <label asp-for="DltAddressWoreda" class="control-label"></label>
                            <input asp-for="DltAddressWoreda" class="form-control" />
                            <span asp-validation-for="DltAddressWoreda" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <label asp-for="DltExpertName" class="control-label"></label>
                            <input asp-for="DltExpertName" class="form-control" />
                            <span asp-validation-for="DltExpertName" class="text-danger"></span>
                        </div>
                   
                        <div class="form-group col-md-4">
                            <label asp-for="DltDoAss" class="control-label"></label>
                                <input type="Date" asp-for="DltDoAss" class="form-control" />
                            <span asp-validation-for="DltDoAss" class="text-danger"></span>
                        </div>
                    </div>
                     <div class="row">
                            <div class="form-group col-md-4">
                                <label asp-for="DltDoRet" class="control-label"></label>
                                <input type="Date" asp-for="DltDoRet" class="form-control" />
                                <span asp-validation-for="DltDoRet" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label asp-for="DltLOS" class="control-label"></label>
                                <input asp-for="DltLOS" class="form-control" />
                                <span asp-validation-for="DltLOS" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label asp-for="CCDltDecissionTypes" class="control-label"></label>
                                <input asp-for="CCDltDecissionTypes" class="form-control" />
                                @* @Html.DropDownList("CCDltDecissionTypes", Html.GetEnumSelectList<CCPDecissionTypes>(), " የአቃቢ ህግ ውሳኔ ይምረጡ ", new { @class = "form-control" }) *@
                                <span asp-validation-for="CCDltDecissionTypes" class="text-danger"></span>
                            </div>
                        </div>
                    
                        <div class="form-group">
                            <label asp-for="DltAssignto" class="control-label"></label>
                            <input asp-for="DltAssignto" class="form-control" />
                            <span asp-validation-for="DltAssignto" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-4">
                            <input type="submit" value="Create" class="btn btn-primary" />
                        </div>
                </form>
                </div>
            </div>
        </div>
    </div>
</div>