@model IEnumerable<ExpertManagmentSystem.Models.CrimeModels.Cr_Decided_Judicial_and_Prosecuter>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
-
<div class="card">
    <div class="card-header">
        <h3 class="card-title">የቀረቡና ውሳኔ የተሰጠባቸው የዐ/ህግ የይግባኝ መዛግብት ዝርዝር ማውጫ</h3>
        <button type="button" class="btn btn-success float-right" data-toggle="modal" data-target="#addDecidednmodals"> <i class="fa fa-plus"></i> አዲስ ጨምር</button>
        @*<a asp-action="Create" class="btn btn-success float-right btn-circle  text-light btn-sm"> አዲስ መዝግብ </a>*@
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <table id="DecidedJudicialAndProsecuterTable" class="table table-bordered table-striped">
            <thead>
        <tr>
            <th>
               ፋይሉ የተከፈተበት ቀን
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProsocuterNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Court_No)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Applicant)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Respondant)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CrimeType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProsecutorComment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Other)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WhoLawyeCommented)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HighCourtDecission)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OtherCourtDecition)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WhoJudgeCommentedOnDecision)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumberOfFemaleAppellants)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumberOfMaleAppellants)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FileStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FileEndResult)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FederalBreakingRequest)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cr_Crime_Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SectrorsDepartment)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OpeningDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProsocuterNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Court_No)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Applicant)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Respondant)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CrimeType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProsecutorComment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Other)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WhoLawyeCommented)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HighCourtDecission)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OtherCourtDecition)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WhoJudgeCommentedOnDecision)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumberOfFemaleAppellants)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumberOfMaleAppellants)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FileStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FileEndResult)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FederalBreakingRequest)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cr_Crime_Type.Cr_Crime_TypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SectrorsDepartment.SectrorsDepartmentId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Cr_Decided_Judicial_and_ProsecuterId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Cr_Decided_Judicial_and_ProsecuterId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Cr_Decided_Judicial_and_ProsecuterId">Delete</a>
            </td>
        </tr>
}
            </tbody>
        </table>
    </div>
</div>


@await Html.PartialAsync("_Cr_Decided_Judicial_and_Prosecuterpartial", new Cr_Decided_Judicial_and_Prosecuter())

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    }