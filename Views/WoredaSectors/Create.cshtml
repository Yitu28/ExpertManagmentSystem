@model ExpertManagmentSystem.OrganizationalStructures.WoredaSectors

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4> የወረዳ ጽህፈት ቤት </h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ZonalSectorsId" class="control-label"></label>
                <select asp-for="ZonalSectorsId" class="form-control" asp-items="ViewBag.ZonalSectorsId"></select>
                <span asp-validation-for="ZonalSectorsId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="WoredaSectorsName" class="control-label"></label>
                <input asp-for="WoredaSectorsName" class="form-control" />
                <span asp-validation-for="WoredaSectorsName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
