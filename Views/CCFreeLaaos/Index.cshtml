                    <th>
                       የፈጀወ ጊዜ
                    </th>
                    <th>
                       የዐቃቢ ህግ ውሳኔ
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.FileNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.RecorNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Applicant)
