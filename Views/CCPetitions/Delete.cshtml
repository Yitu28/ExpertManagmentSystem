                    <div class="form-group col-md-3">
                        <label asp-for="PeDoariv" class="control-label"></label>
                        <input asp-for="PeDoariv" class="form-control" disabled />
                        <span asp-validation-for="PeDoariv" class="text-danger"></span>
                        <span asp-validation-for="PeDoariv" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-3">
                        <label asp-for="ZonalSectorsId" class="control-label"></label>
                        <input asp-for="ZonalSectorsId" class="form-control" disabled />
                        <span asp-validation-for="ZonalSectorsId" class="text-danger"></span>
                    </div>
                </div>
                <div class="row">
                    
                    <div class="form-group col-md-3">
                        <label asp-for="WoredaSectorsId" class="control-label"></label>
                        <input asp-for="WoredaSectorsId" class="form-control" disabled />
                        <span asp-validation-for="WoredaSectorsId" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-3">
                        <label asp-for="PeExpertName" class="control-label"></label>
                        <input asp-for="PeExpertName" class="form-control" disabled />
                        <span asp-validation-for="PeExpertName" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-3">
                        <label asp-for="PeDaos" class="control-label"></label>
                        <input asp-for="PeDaos" class="form-control" disabled />
                        <span asp-validation-for="PeDaos" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-3">
                        <label asp-for="ZonalSectorsId" class="control-label"></label>
                        <input asp-for="ZonalSectorsId" class="form-control" disabled />
                        <span asp-validation-for="ZonalSectorsId" class="text-danger"></span>
                    </div>
                </div>
                <div class="row">
                   
                    <div class="form-group col-md-3">
                        <label asp-for="WoredaSectorsId" class="control-label"></label>
                        <input asp-for="WoredaSectorsId" class="form-control" disabled />
                        <span asp-validation-for="WoredaSectorsId" class="text-danger"></span>
                    </div>
                    <div class="form-group col-md-3">
                        <label asp-for="Petitionrespons" class="control-label"></label>
                        @Html.DropDownList("Petitionrespons", Html.GetEnumSelectList<Petitionrespons>(), " የአቤቱታው የቀረበበትን ሁኔታ ይምረጡ ", new { @class = "form-control" })
